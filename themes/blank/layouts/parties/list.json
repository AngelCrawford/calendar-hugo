{{- $.Scratch.Add "partiesList" slice -}}
{{- range where .Site.RegularPages "Section" "=" "parties" -}}

    {{- $.Scratch.Add "partiesList" (dict "title" .Title "start" .Params.StartDate "end" .Params.endDate "content" .Summary "url" .Permalink "city" .Params.city "kind" .Params.kind) -}}
{{- end -}}
{{- $.Scratch.Get "partiesList" | jsonify -}}