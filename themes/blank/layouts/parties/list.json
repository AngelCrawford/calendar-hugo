{{- $.Scratch.Add "partiesList" slice -}}
{{- range where .Site.RegularPages "Section" "=" "parties" -}}
    
    {{- $.Scratch.Add "partiesList" (dict "title" .Title "start" .Date "tags" .Params.tags "content" .Summary "permalink" .Permalink) -}}
{{- end -}}
{{- $.Scratch.Get "partiesList" | jsonify -}}